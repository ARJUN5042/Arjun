  	<init-param>
  		<param-name>password</param-name>
  		<param-value>Arjun123</param-value>
  	</init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Arjun</servlet-name>
  	<url-pattern>/servletconfigdemo</url-pattern>
  </servlet-mapping>
  
</web-app>